{
  "editor.minimap.scale": 2,
  "editor.minimap.sectionHeaderFontSize": 10,
  "editor.minimap.size": "proportional",
  "editor.inlineSuggest.suppressSuggestions": false,
  "editor.suggest.localityBonus": false,
  "editor.suggest.preview": false,
  "php-docblocker.alignParams": true,
  "php-docblocker.alignReturn": true,
  "php-docblocker.qualifyClassNames": true,
  "php-docblocker.returnGap": true,
  "php-docblocker.useShortNames": true,
  "php-docblocker.author": {
    "name": "Galactic-Shrine",
    "email": "Support@Galactic-Shrine.com"
  },
  "material-icon-theme.activeIconPack": "angular",
  "material-icon-theme.folders.color": "#ef5350",
  "material-icon-theme.folders.associations": {
    "migrations": "Database",
    "Migrations": "Database",
    "OAuth": "Secure",
    "Oauth": "Secure",
    "oauth": "Secure",
    "Files": "Content",
    "files": "Content",
    "Applications": "App",
    "applications": "App",
    "Forums": "Resource",
    "forums": "Resource",
    "Gallery": "Components",
    "gallery": "Components",
    "Users": "Client",
    "users": "Client"
  },
  "workbench.iconTheme": "material-icon-theme",
  "php.version": "8.3",
  "CodeGPT.apiKey": "CodeGPT Plus Beta",
  "liveSassCompile.settings.formats": [
    // This is the default.
    {
        "format": "expanded",
        "extensionName": ".css",

        // null for all three -> denotes the same path as the SASS file
        "savePathReplacementPairs": {
          "/assets/Scss": "/assets/Css"
      }
    },
    // You can add more
    {
        "format": "compressed",
        "extensionName": ".min.css",

        // / -> denotes relative to the workspace root
        "savePathReplacementPairs": {
          "/assets/Scss": "/assets/Css"
      }
    }
  ],
  "liveSassCompile.settings.partialsList": [
    "/**/_*.s[ac]ss",
    "/_Partials/_*.s[ac]ss",
    "/**/_Partials/_*.s[ac]ss",
    "/assets/Scss/_Partials/_*.s[ac]ss",
    "/**/assets/Scss/_Partials/_*.s[ac]ss"
  ],
  "liveSassCompile.settings.watchOnLaunch": true,
  "liveSassCompile.settings.excludeList": [
    "/**/node_modules/**",
    "/.vscode/**",
    "/.git/**"
  ],
  "liveSassCompile.settings.generateMap": true,
}